.meteor {
  display: block;
  width: 2rpx;
  height: 2rpx;
  border-radius: 50%;
  background: #FFF;
  top: 100rpx;
  left: 400rpx;
  position: absolute;
  transform-origin: 100% 0;
  animation: star-ani 6s infinite ease-out;
  box-shadow: 0 0 2rpx 2rpx #f9e87c;
  opacity: 0;
  z-index: 2; }

.meteor::after {
  content: '';
  display: block;
  top: 2rpx;
  left: 4rpx;
  border: 0rpx solid #fff;
  border-width: 0rpx 90rpx 5rpx 90rpx;
  border-color: transparent transparent transparent rgba(249, 232, 124, 0.8);
  transform: rotate(-45deg) translate3d(2rpx, 1rpx, 0);
  box-shadow: 0 0 1rpx 0 rgba(249, 232, 124, 0.1);
  transform-origin: 0% 100%;
  animation: shooting-ani 3s infinite ease-out; }

